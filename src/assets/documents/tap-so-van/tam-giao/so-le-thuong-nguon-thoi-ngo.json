                        "content": [
                            {
                                "type": "text",
                                "text": "\t\t\tViết:\tCao-Đài Tiên-Ông Đại-Bồ-Tát Ma-Ha-Tát.\n"
                            }
                        ],
                        "key": "so-le-thuong-nguon-thoi-ngo-6p0",
