                        },
                        "content": [
                            {
                                "type": "text",
                                "text": "TAM TÔNG CHƠN-GIÁO",
                                "attrs": {
                                    "fontWeight": "bold"
                                }
                            }
                        ],
                        "key": "so-le-ky-niem-duc-thuong-chuong-phap-8p0",
