                        "content": [
                            {
                                "type": "text",
                                "text": "\t\t\tViết:\tCao-Đài Tiên-Ông Đại-Bồ-Tát Ma-Ha-Tát.\n"
                            }
                        ],
                        "key": "so-le-ky-niem-duc-thuong-chuong-phap-6p0",
