{
  "name": "pwa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-dev": "ng serve",
    "start": "node server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start-pwa": "ng build && cd dist/pwa && copy index.html 404.html && http-server -p 8080 -c-1 dist/pwa",
    "gh-pages-build": "ng deploy --dry-run",
    "deploy-to-firebase": "ng deploy --dry-run --base-href=https://www.caodaion.com && cd dist/pwa && copy index.html 404.html && firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.4",
    "@angular/cdk": "^15.0.3",
    "@angular/common": "^15.0.4",
    "@angular/compiler": "^15.0.4",
    "@angular/core": "^15.0.4",
    "@angular/forms": "^15.0.4",
    "@angular/material": "^15.0.3",
    "@angular/platform-browser": "^15.0.4",
    "@angular/platform-browser-dynamic": "^15.0.4",
    "@angular/router": "^15.0.4",
    "@angular/service-worker": "^15.0.4",
    "@auth0/angular-jwt": "^5.1.2",
    "@types/crypto-js": "^4.1.1",
    "angularx-qrcode": "^15.0.1",
    "crypto-js": "^4.1.1",
    "express": "^4.18.2",
    "path": "^0.12.7",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "^15.0.4",
    "@angular/compiler-cli": "^15.0.4",
    "@types/jasmine": "~3.10.7",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.5",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.2",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.8.4"
  },
  "engines": {
    "node": "16.14.0",
    "npm": "8.3.1"
  }
